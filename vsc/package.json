{
  "name": "locks-language",
  "displayName": "Locks Language",
  "description": "An extension for the locks language",
  "version": "1.0.0",
  "publisher": "kyleect",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kyleect/locks"
  },
  "bugs": {
    "url": "https://github.com/kyleect/locks/issues"
  },
  "engines": {
    "vscode": "^1.83.1"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "main": "out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "locks",
        "aliases": [
          "locks"
        ],
        "extensions": [
          ".locks"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "locks",
        "scopeName": "source.locks",
        "path": "./locks.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "locks",
        "path": "./snippets.json"
      }
    ],
    "configuration": {
      "title": "Locks",
      "properties": {
        "locks.binPath": {
          "type": [
            "string",
            "null"
          ],
          "default": null,
          "description": "Specifies the folder path containing the locks binary."
        }
      }
    },
    "commands": [
      {
        "command": "locks.startLanguageServer",
        "title": "Start Locks Language Server"
      },
      {
        "command": "locks.stopLanguageServer",
        "title": "Stop Locks Language Server"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^17.0.45",
    "@types/vscode": "^1.83.1",
    "@vscode/test-electron": "2.3.6",
    "typescript": "^4.6.4"
  },
  "dependencies": {
    "vscode-languageclient": "9.0.1"
  },
  "scripts": {
    "prepublish": "tsc -p",
    "build": "tsc",
    "watch": "tsc -w",
    "update-vscode": "node ./node_modules/vscode/bin/install"
  }
}
