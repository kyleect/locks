class Base {
  fn method() {
    print("Base.method()");
  }
}

class Derived extends Base {
  fn method() {
    super.method();
  }
}

class OtherBase {
  fn method() {
    print("OtherBase.method()");
  }
}

let derived = Derived();
derived.method(); // out: Base.method()
Base = OtherBase;
derived.method(); // out: Base.method()
