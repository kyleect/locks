{
  "Function": {
    "prefix": "fun",
    "body": ["fun $1($2) {", "  $3", "}$4"],
    "description": "Create a function"
  },
  "Class": {
    "prefix": "class",
    "body": ["class $1 {", "  $2", "}$3"],
    "description": "Define a class"
  },
  "Variable": {
    "prefix": "var",
    "body": ["var $1 = $2;$3"],
    "description": "Define a variable"
  },
  "For": {
    "prefix": "for",
    "body": ["for (var $1 = 0; $1 < $2; $1 = $1 + 1) {", "  $3", "}$4"],
    "description": "Create a basic for loop"
  },
  "If": {
    "prefix": "if",
    "body": ["if ($1) {", "  $2", "}$3"],
    "description": "Create a basic if statement"
  },
  "IfElse": {
    "prefix": "ifelse",
    "body": ["if ($1) {", "  $2", "} else {", "  $3", "}$4"],
    "description": "Create an if/else statement"
  },
  "Print": {
    "prefix": "print",
    "body": ["print $1;$2"],
    "description": "Print value"
  }
}
