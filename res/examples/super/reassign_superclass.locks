class Base {
  method() {
    print "Base.method()";
  }
}

class Derived extends Base {
  method() {
    super.method();
  }
}

class OtherBase {
  method() {
    print "OtherBase.method()";
  }
}

let derived = Derived();
derived.method(); // out: Base.method()
Base = OtherBase;
derived.method(); // out: Base.method()
